{
  "name": "xmd2html",
  "version": "1.0.0",
  "description": "A parser from eXtended MarkDown to HTML",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-second-trial/xmd2html.git"
  },
  "keywords": [
    "parser",
    "html",
    "markdown"
  ],
  "author": "Andrea Tino",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/the-second-trial/xmd2html/issues"
  },
  "homepage": "https://github.com/the-second-trial/xmd2html#readme",
  "scripts": {
    "build": "npm run build:xmdparser && npm run build:grammar && npm run build:pysrv && npm run build:res",
    "build:xmdparser": "npm run build --prefix ./xmdparser && node scripts/copy_xmdparser.js",
    "build:grammar": "node scripts/copy_grammar.js",
    "build:pysrv": "node scripts/copy_pysrv.js",
    "build:res": "node scripts/copy_res.js",
    "clean": "node scripts/clean.js",
    "start": "node ./lib/xmlparser.js --src ./examples/simple.md"
  }
}
