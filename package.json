{
  "name": "xmd2html",
  "version": "1.0.0",
  "description": "A parser from eXtended MarkDown to HTML",
  "scripts": {
    "build": "npm run build:bundlets && npm run build:copypysrv",
    "build:bundlets": "webpack --mode=development",
    "build:copypysrv": "node scripts/copy_pysrv.js",
    "start": "node ./src/index.js --src ./examples/simple.md",
    "test": "jest --config=jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/the-second-trial/xmd2html.git"
  },
  "keywords": [
    "parser",
    "html",
    "markdown"
  ],
  "author": "Andrea Tino",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/the-second-trial/xmd2html/issues"
  },
  "homepage": "https://github.com/the-second-trial/xmd2html#readme",
  "dependencies": {
    "command-line-args": "5.2.1",
    "pegjs": "0.10.0",
    "node-fetch": "3.2.3"
  },
  "devDependencies": {
    "jest": "27.5.1",
    "typescript": "4.6.3",
    "ts-loader": "9.2.8",
    "webpack-cli": "4.9.2",
    "@types/command-line-args": "5.2.0",
    "@types/pegjs": "0.10.3"
  }
}
